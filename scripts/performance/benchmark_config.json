{
  "benchmark_type": "comprehensive",
  "enable_monitoring": true,
  "enable_optimization": false,
  "monitoring_duration": 300,
  "optimization_experiments": 20,
  "output_format": "both",
  "parallel_execution": true,
  "alert_thresholds": {
    "error_rate_max": 0.05,
    "latency_p95_max": 2.0,
    "latency_p99_max": 5.0,
    "cpu_usage_max": 85.0,
    "memory_usage_max": 90.0,
    "throughput_min": 100.0,
    "quantum_scalability_min": 0.7
  },
  "load_test_scenarios": [
    {
      "name": "Baseline Load",
      "description": "Baseline performance test with minimal load",
      "duration": 60,
      "concurrent_users": 5,
      "ramp_up_time": 15,
      "think_time": 2.0,
      "endpoints": [
        "/api/v1/health",
        "/api/v1/status"
      ],
      "request_patterns": {
        "distribution": "constant",
        "weight": 1.0
      }
    },
    {
      "name": "Light Load",
      "description": "Light production load simulation",
      "duration": 120,
      "concurrent_users": 10,
      "ramp_up_time": 30,
      "think_time": 1.5,
      "endpoints": [
        "/api/v1/health",
        "/api/v1/projects",
        "/api/v1/tasks",
        "/api/v1/users"
      ],
      "request_patterns": {
        "distribution": "weighted",
        "weights": {
          "/api/v1/health": 0.1,
          "/api/v1/projects": 0.4,
          "/api/v1/tasks": 0.4,
          "/api/v1/users": 0.1
        }
      }
    },
    {
      "name": "Medium Load",
      "description": "Medium production load with realistic usage patterns",
      "duration": 180,
      "concurrent_users": 50,
      "ramp_up_time": 60,
      "think_time": 1.0,
      "endpoints": [
        "/api/v1/health",
        "/api/v1/projects",
        "/api/v1/tasks",
        "/api/v1/users",
        "/api/v1/analytics",
        "/api/v1/workflows"
      ],
      "request_patterns": {
        "distribution": "weighted",
        "weights": {
          "/api/v1/health": 0.05,
          "/api/v1/projects": 0.25,
          "/api/v1/tasks": 0.30,
          "/api/v1/users": 0.15,
          "/api/v1/analytics": 0.15,
          "/api/v1/workflows": 0.10
        }
      }
    },
    {
      "name": "Heavy Load",
      "description": "Heavy production load testing system limits",
      "duration": 300,
      "concurrent_users": 100,
      "ramp_up_time": 120,
      "think_time": 0.5,
      "endpoints": [
        "/api/v1/health",
        "/api/v1/projects",
        "/api/v1/tasks",
        "/api/v1/users",
        "/api/v1/analytics",
        "/api/v1/workflows",
        "/api/v1/ai/generate",
        "/api/v1/community/templates"
      ],
      "request_patterns": {
        "distribution": "weighted",
        "weights": {
          "/api/v1/health": 0.02,
          "/api/v1/projects": 0.20,
          "/api/v1/tasks": 0.25,
          "/api/v1/users": 0.10,
          "/api/v1/analytics": 0.15,
          "/api/v1/workflows": 0.10,
          "/api/v1/ai/generate": 0.13,
          "/api/v1/community/templates": 0.05
        }
      }
    },
    {
      "name": "Peak Load",
      "description": "Peak load testing for extreme scenarios",
      "duration": 240,
      "concurrent_users": 200,
      "ramp_up_time": 180,
      "think_time": 0.2,
      "endpoints": [
        "/api/v1/health",
        "/api/v1/projects",
        "/api/v1/tasks",
        "/api/v1/users",
        "/api/v1/analytics",
        "/api/v1/workflows",
        "/api/v1/ai/generate",
        "/api/v1/community/templates",
        "/api/v1/performance/metrics"
      ],
      "request_patterns": {
        "distribution": "weighted",
        "weights": {
          "/api/v1/health": 0.01,
          "/api/v1/projects": 0.18,
          "/api/v1/tasks": 0.22,
          "/api/v1/users": 0.08,
          "/api/v1/analytics": 0.15,
          "/api/v1/workflows": 0.12,
          "/api/v1/ai/generate": 0.15,
          "/api/v1/community/templates": 0.06,
          "/api/v1/performance/metrics": 0.03
        }
      }
    },
    {
      "name": "Spike Test",
      "description": "Traffic spike simulation",
      "duration": 120,
      "concurrent_users": 300,
      "ramp_up_time": 30,
      "think_time": 0.1,
      "endpoints": [
        "/api/v1/health",
        "/api/v1/projects",
        "/api/v1/ai/generate"
      ],
      "request_patterns": {
        "distribution": "spike",
        "spike_pattern": {
          "initial_load": 0.1,
          "spike_multiplier": 10.0,
          "spike_duration": 60,
          "recovery_time": 30
        }
      }
    }
  ],
  "quantum_intelligence_config": {
    "modules": [
      "quantum_consciousness",
      "quantum_memory",
      "quantum_reasoning",
      "quantum_optimization"
    ],
    "load_levels": [1, 5, 10, 25, 50, 100],
    "test_duration_per_level": 30,
    "scalability_threshold": 0.7,
    "performance_targets": {
      "quantum_consciousness": {
        "min_ops_per_second": 50,
        "max_latency_ms": 200
      },
      "quantum_memory": {
        "min_ops_per_second": 100,
        "max_latency_ms": 100
      },
      "quantum_reasoning": {
        "min_ops_per_second": 30,
        "max_latency_ms": 500
      },
      "quantum_optimization": {
        "min_ops_per_second": 40,
        "max_latency_ms": 300
      }
    }
  },
  "database_performance_config": {
    "connection_pool_sizes": [5, 10, 20, 50, 100],
    "query_types": [
      "simple_select",
      "complex_join",
      "aggregation",
      "full_text_search",
      "transaction_heavy"
    ],
    "cache_configurations": [
      {
        "name": "redis_default",
        "size_mb": 256,
        "eviction_policy": "lru"
      },
      {
        "name": "redis_optimized",
        "size_mb": 512,
        "eviction_policy": "lfu"
      },
      {
        "name": "memory_cache",
        "size_mb": 128,
        "eviction_policy": "ttl"
      }
    ],
    "performance_targets": {
      "max_query_time_ms": 100,
      "min_cache_hit_rate": 0.8,
      "max_connection_wait_ms": 50
    }
  },
  "monitoring_config": {
    "metrics_collection_interval": 5.0,
    "dashboard_update_interval": 10.0,
    "data_retention_hours": 168,
    "alert_evaluation_interval": 15.0,
    "sla_thresholds": [
      {
        "metric_name": "cpu.percent",
        "threshold_value": 80.0,
        "comparison": "lt",
        "severity": "warning",
        "description": "CPU usage should be below 80%"
      },
      {
        "metric_name": "memory.percent",
        "threshold_value": 85.0,
        "comparison": "lt",
        "severity": "critical",
        "description": "Memory usage should be below 85%"
      },
      {
        "metric_name": "response_time.p95",
        "threshold_value": 2.0,
        "comparison": "lt",
        "severity": "warning",
        "description": "P95 response time should be below 2s"
      },
      {
        "metric_name": "response_time.p99",
        "threshold_value": 5.0,
        "comparison": "lt",
        "severity": "critical",
        "description": "P99 response time should be below 5s"
      },
      {
        "metric_name": "error_rate",
        "threshold_value": 0.05,
        "comparison": "lt",
        "severity": "critical",
        "description": "Error rate should be below 5%"
      },
      {
        "metric_name": "throughput",
        "threshold_value": 100.0,
        "comparison": "gt",
        "severity": "warning",
        "description": "Throughput should be above 100 ops/s"
      }
    ],
    "alert_rules": [
      {
        "name": "high_cpu",
        "condition": "cpu.percent",
        "threshold": 90.0,
        "duration": 30,
        "severity": "critical",
        "action": "scale_up"
      },
      {
        "name": "high_memory",
        "condition": "memory.percent",
        "threshold": 90.0,
        "duration": 30,
        "severity": "critical",
        "action": "restart_service"
      },
      {
        "name": "high_error_rate",
        "condition": "error_rate",
        "threshold": 0.1,
        "duration": 60,
        "severity": "critical",
        "action": "investigate"
      },
      {
        "name": "low_throughput",
        "condition": "throughput",
        "threshold": 50.0,
        "duration": 120,
        "severity": "warning",
        "action": "optimize"
      },
      {
        "name": "high_latency",
        "condition": "response_time.p95",
        "threshold": 3.0,
        "duration": 60,
        "severity": "warning",
        "action": "investigate"
      },
      {
        "name": "database_slow",
        "condition": "db.query_time.avg",
        "threshold": 200.0,
        "duration": 120,
        "severity": "warning",
        "action": "optimize_queries"
      }
    ]
  },
  "optimization_config": {
    "enabled": false,
    "max_experiments": 20,
    "experiment_duration": 60,
    "convergence_threshold": 0.05,
    "safety_mode": true,
    "parameters": [
      {
        "name": "db_pool_size",
        "current_value": 20,
        "min_value": 5,
        "max_value": 100,
        "step_size": 5,
        "parameter_type": "int",
        "description": "Database connection pool size"
      },
      {
        "name": "cache_size_mb",
        "current_value": 256,
        "min_value": 64,
        "max_value": 2048,
        "step_size": 64,
        "parameter_type": "int",
        "description": "Cache size in megabytes"
      },
      {
        "name": "worker_threads",
        "current_value": 8,
        "min_value": 2,
        "max_value": 32,
        "step_size": 2,
        "parameter_type": "int",
        "description": "Number of worker threads"
      },
      {
        "name": "request_timeout",
        "current_value": 30.0,
        "min_value": 5.0,
        "max_value": 120.0,
        "step_size": 5.0,
        "parameter_type": "float",
        "description": "Request timeout in seconds"
      },
      {
        "name": "batch_size",
        "current_value": 100,
        "min_value": 10,
        "max_value": 1000,
        "step_size": 10,
        "parameter_type": "int",
        "description": "Batch processing size"
      },
      {
        "name": "compression_enabled",
        "current_value": true,
        "min_value": false,
        "max_value": true,
        "step_size": null,
        "parameter_type": "bool",
        "description": "Enable response compression"
      }
    ],
    "optimization_targets": [
      {
        "name": "throughput",
        "metric_path": "throughput",
        "target_value": 1000.0,
        "optimization_direction": "maximize",
        "weight": 0.4,
        "constraint_min": null,
        "constraint_max": null
      },
      {
        "name": "latency_p95",
        "metric_path": "response_time.p95",
        "target_value": 1.0,
        "optimization_direction": "minimize",
        "weight": 0.3,
        "constraint_min": null,
        "constraint_max": 10.0
      },
      {
        "name": "error_rate",
        "metric_path": "error_rate",
        "target_value": 0.01,
        "optimization_direction": "minimize",
        "weight": 0.2,
        "constraint_min": null,
        "constraint_max": 0.1
      },
      {
        "name": "cpu_usage",
        "metric_path": "cpu.percent",
        "target_value": 70.0,
        "optimization_direction": "minimize",
        "weight": 0.1,
        "constraint_min": null,
        "constraint_max": 90.0
      }
    ]
  },
  "reporting_config": {
    "generate_html_report": true,
    "generate_json_report": true,
    "generate_executive_summary": true,
    "include_charts": true,
    "include_recommendations": true,
    "email_notifications": false,
    "slack_notifications": false,
    "webhook_urls": [],
    "report_retention_days": 30
  },
  "environment_config": {
    "target_environment": "production",
    "api_base_url": "http://localhost:8000",
    "health_check_endpoint": "/api/v1/health",
    "metrics_endpoint": "/api/v1/performance/metrics",
    "required_services": [
      {
        "name": "api_server",
        "host": "localhost",
        "port": 8000,
        "health_check": "/health"
      },
      {
        "name": "database",
        "host": "localhost",
        "port": 5432,
        "type": "postgresql"
      },
      {
        "name": "redis",
        "host": "localhost",
        "port": 6379,
        "type": "redis"
      }
    ]
  }
}