{
  "dashboard": {
    "id": null,
    "title": "Claude TUI - Performance Monitoring Dashboard",
    "tags": ["claude-tui", "performance", "monitoring"],
    "timezone": "UTC",
    "editable": true,
    "hideControls": false,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"claude-tui\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "claude_tui_system_health_score",
            "legendFormat": "Health Score"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 85
                }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "auto"
        }
      },
      {
        "id": 2,
        "title": "Task Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(claude_tui_tasks_completed_total[5m]) / rate(claude_tui_tasks_total[5m]) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Average Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 200
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Active Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "claude_tui_active_agents",
            "legendFormat": "Active Agents"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "green",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\", instance=~\"$instance\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [85],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High CPU Usage Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes{instance=~\"$instance\"} / node_memory_MemTotal_bytes{instance=~\"$instance\"})) * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ]
      },
      {
        "id": 7,
        "title": "Request Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "reqps"
          }
        ]
      },
      {
        "id": 8,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) * 1000",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) * 1000",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) * 1000",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "ms"
          }
        ]
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\", instance=~\"$instance\"}[5m]) / rate(http_requests_total{instance=~\"$instance\"}[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "percent"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "3m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert"
        }
      },
      {
        "id": 10,
        "title": "Task Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "claude_tui_task_queue_depth{instance=~\"$instance\"}",
            "legendFormat": "Queue Depth"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "short"
          }
        ]
      },
      {
        "id": 11,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(database_queries_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Queries/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket{instance=~\"$instance\"}[5m])) * 1000",
            "legendFormat": "P95 Query Time (ms)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "short"
          },
          {
            "min": 0,
            "unit": "ms"
          }
        ]
      },
      {
        "id": 12,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cache_hits_total{instance=~\"$instance\"}[5m]) / (rate(cache_hits_total{instance=~\"$instance\"}[5m]) + rate(cache_misses_total{instance=~\"$instance\"}[5m])) * 100",
            "legendFormat": "Hit Rate %"
          },
          {
            "expr": "cache_memory_used_bytes{instance=~\"$instance\"} / cache_memory_max_bytes{instance=~\"$instance\"} * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ]
      },
      {
        "id": 13,
        "title": "Auto-scaling Events",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(kubernetes_hpa_scale_events_total{instance=~\"$instance\"}[1h])",
            "legendFormat": "Scaling Events (1h)"
          },
          {
            "expr": "kubernetes_hpa_replicas{instance=~\"$instance\"}",
            "legendFormat": "Current Replicas"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "short"
          }
        ]
      },
      {
        "id": 14,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total{instance=~\"$instance\", device!=\"lo\"}[5m]) * 8",
            "legendFormat": "Receive {{device}}"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total{instance=~\"$instance\", device!=\"lo\"}[5m]) * 8",
            "legendFormat": "Transmit {{device}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        },
        "yAxes": [
          {
            "min": 0,
            "unit": "bps"
          }
        ]
      },
      {
        "id": 15,
        "title": "SLA Compliance",
        "type": "table",
        "targets": [
          {
            "expr": "claude_tui_sla_availability",
            "legendFormat": "Availability"
          },
          {
            "expr": "claude_tui_sla_response_time_compliance",
            "legendFormat": "Response Time"
          },
          {
            "expr": "claude_tui_sla_error_rate_compliance",
            "legendFormat": "Error Rate"
          },
          {
            "expr": "claude_tui_sla_throughput_compliance",
            "legendFormat": "Throughput"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 44
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 90,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 90
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(up{job=\"claude-tui\"}[1m]) > 0",
          "iconColor": "blue",
          "titleFormat": "Deployment"
        },
        {
          "name": "Alerts",
          "datasource": "Alertmanager",
          "enable": true,
          "iconColor": "red",
          "titleFormat": "Alert: {{alertname}}"
        }
      ]
    }
  }
}