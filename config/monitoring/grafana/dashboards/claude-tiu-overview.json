{
  "dashboard": {
    "id": null,
    "title": "Claude TIU - Application Overview",
    "tags": ["claude-tiu", "overview"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "sharedCrosshair": false,
    "panels": [
      {
        "id": 1,
        "title": "Application Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"claude-tiu-app\"}",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "{{instance}} - {{method}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th Percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"claude-tiu-app\"}[5m]))",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"claude-tiu-app\"}[5m]))",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"claude-tiu-app\",status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "5xx Error Rate"
          },
          {
            "expr": "rate(http_requests_total{job=\"claude-tiu-app\",status=~\"4..\"}[5m]) / rate(http_requests_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "4xx Error Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"claude-tiu-app\"}[5m]) * 100",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "{{instance}}"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"claude-tiu-app\"} / 1024 / 1024",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "{{instance}} RSS"
          },
          {
            "expr": "process_virtual_memory_bytes{job=\"claude-tiu-app\"} / 1024 / 1024",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "{{instance}} Virtual"
          }
        ],
        "yAxes": [
          {
            "label": "MB",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{job=\"postgres-exporter\"}",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "pg_settings_max_connections{job=\"postgres-exporter\"}",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "Max Connections"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Redis Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes{job=\"redis-exporter\"} / 1024 / 1024",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "Used Memory (MB)"
          },
          {
            "expr": "redis_memory_max_bytes{job=\"redis-exporter\"} / 1024 / 1024",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "Max Memory (MB)"
          }
        ],
        "yAxes": [
          {
            "label": "MB",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "AI Service Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(claude_tiu_ai_requests_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "AI Requests/sec"
          },
          {
            "expr": "rate(claude_tiu_ai_requests_timeout_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "AI Timeouts/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Rate",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Project Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(claude_tiu_project_creation_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "A",
            "legendFormat": "Projects Created/sec"
          },
          {
            "expr": "rate(claude_tiu_project_creation_failures_total{job=\"claude-tiu-app\"}[5m])",
            "format": "time_series",
            "refId": "B",
            "legendFormat": "Creation Failures/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Rate",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1
  }
}
